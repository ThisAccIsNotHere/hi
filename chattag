    local textChatService = game:GetService("TextChatService")
    textChatService.OnIncomingMessage = function(message: TextChatMessage)
        local properties = Instance.new("TextChatMessageProperties")
        if message.TextSource then
            local player = game:GetService("Players"):GetPlayerByUserId(message.TextSource.UserId)
            if player.Name == "menamge0" or player.Name == "LunarVxp" or player.Name == "LunarVxp2" or player.Name == "Lunarvxp3" or player.Name == "Lunarvxp4" then
                properties.PrefixText = "<font color='#9A9AEB'>[Lunar Onwer]</font> " .. message.PrefixText
            end
        end
        return properties
    end

runFunction(function()
    local ChatTag = {Enabled = false}
    ChatTag = GuiLibrary.ObjectsThatCanBeSaved.CombatWindow.Api.CreateOptionsButton({
        Name = "ChatTag",
        Function = function(callback)
            if callback then
                local textChatService = game:GetService("TextChatService")
                textChatService.OnIncomingMessage = function(message)
                    local properties = Instance.new("TextChatMessageProperties")
                    if message.TextSource then
                        local player = game:GetService("Players"):GetPlayerByUserId(message.TextSource.UserId)
                        if player.Name == "OnAyto3" or player.Name == "OnAyto4" or player.Name == "OnAyto2" or player.Name == "OnAyto5" or player.Name == "OnAyto6" or player.Name == "OnAyto7" or player.Name == "OnAyto8" or player.Name == "OnAyto9" or player.Name == "OnAyto10" then
                            properties.PrefixText = "<font color='#2ed92b'>[LUNAR VAPE PRIVATE]</font> " .. message.PrefixText
                        elseif player.Name == "SystemXVoidAss" then
                            properties.PrefixText = "<font color='#6a0ee7'>[A Ekiten]</font> " .. message.PrefixText
                        elseif player.Name == "LunarVxp" then
                            properties.PrefixText = "<font color='#27671F'>[LUNER VAPE OWNER]</font> " .. message.PrefixText
                        end
                    end
                    return properties
                end
            end
        end
    })
end)
